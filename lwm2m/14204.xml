<?xml version="1.0" encoding="UTF-8"?>
<LWM2M
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
	<Object ObjectType="MODefinition">
		<Name>Device information</Name>
		<Description1>
			<![CDATA[Details about the device's connection.]]>
		</Description1>
		<ObjectID>14204</ObjectID>
		<ObjectURN>urn:oma:lwm2m:x:14204</ObjectURN>
						<MultipleInstances>Single</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
            <Item ID="0">
                <Name>IMEI</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Board IMEI. Examples: 352656106111232.]]>
				</Description>
            </Item>
            <Item ID="1">
                <Name>SIM ICCID</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Examples: 89450421180216216095.]]>
				</Description>
            </Item>
            <Item ID="2">
                <Name>Modem firmware version</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Examples: mfw_nrf9160_1.0.0.]]>
				</Description>
            </Item>
            <Item ID="3">
                <Name>Application firmware version</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Examples: v1.0.0-rc1-327-g6fc8c16b239f.]]>
				</Description>
            </Item>
            <Item ID="4">
                <Name>Board version</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Examples: thingy91_nrf9160.]]>
				</Description>
            </Item>
            <Item ID="5">
                <Name>Battery model</Name>
                <Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[Examples: LP302535, LP502540, LP803035.]]>
				</Description>
            </Item>
			<Item ID="99">
				<Name>Timestamp</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Time</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>
					<![CDATA[The timestamp of when the measurement was performed.]]>
				</Description>
			</Item>
		</Resources>
		<Description2></Description2>
	</Object>
</LWM2M>
